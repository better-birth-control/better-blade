{
    "name": "betterbirthcontrol/better-blade",
    "description": "",
    "keywords": [
        "betterbirthcontrol",
        "better-blade"
    ],
    "homepage": "https://github.com/betterbirthcontrol/better-blade",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ben",
            "email": "ben@betterbirthcontrol.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/view": "^9.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "BetterBirthControl\\Better\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BetterBirthControl\\Better\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BetterBirthControl\\Better\\BetterServiceProvider"
            ]
        }
    }
}
